<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!--Текущая скорость чтения символов в минуту-->
		<add key="Speed" value="1200" />
		<!--Максимальная скорость чтения символов в минуту-->
		<add key="MaxSpeed" value="12000" />
		<!--Интервал между повышением скорости чтения на один символ-->
		<add key="TimeToUpSpeed" value="120000" />
		<!--Последний открытый файл-->
		<add key="LastFile" value="Not File" />
		<!--Последняя позиция-->
		<add key="LastPosition" value="1" />
		<!--Опция повышения скорости-->
		<add key="SpeedUp" value="true" />
	</appSettings>
	<startup> 
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
</configuration>